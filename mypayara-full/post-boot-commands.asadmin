add-library  /tmp/postgresql.jar
set-hazelcast-configuration --enabled=true --dynamic=true
create-jdbc-connection-pool --datasourceclassname org.postgresql.ds.PGConnectionPoolDataSource --restype javax.sql.ConnectionPoolDataSource --property password=root:user=root:databaseName=postgres:url="jdbc:postgresql://db/postgres" pp-pool
create-jdbc-resource --connectionpoolid pp-pool jdbc/postgresql-pool
